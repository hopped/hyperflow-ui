{
  "processes": [
    {
      "name": "run-cmd-openmp",
      "function": "amqpCommand",
      "type": "dataflow",
      "config": {
        "executor": {
          "executable": "/opt/shared/MD_v4_MPI/run-cmd.sh > run.log 2>&1",
          "args": [
            "1000",
            "0.5",
            "0.5"
          ]
        }
      },
      "ins": [
        "start"
      ],
      "outs": [
        "/opt/shared/{{ .FilenameOutArchived }}"
      ]
    },
    {
      "name": "make-movie",
      "function": "amqpCommand",
      "type": "dataflow",
      "config": {
        "executor": {
          "executable": "/opt/shared/MD_v4_MPI/make-movie.sh > movie.log 2>&1",
          "args": [
            "/opt/shared/{{ .FilenameOutArchived }}",
            "/opt/shared/{{ .FilenameOutVideo }}"
          ]
        }
      },
      "ins": [
        "/opt/shared/{{ .FilenameOutArchived }}"
      ],
      "outs": [
        "/opt/shared/{{ .FilenameOutVideo }}"
      ]
    }
  ],
  "signals": [
    {
      "name": "start",
      "data": [
        "start"
      ]
    },
    {
      "name": "/opt/shared/{{ .FilenameOutArchived }}"
    },
    {
      "name": "/opt/shared/{{ .FilenameOutVideo }}"
    }
  ],
  "ins": [
    "start"
  ],
  "outs": [
    "/opt/shared/{{ .FilenameOutVideo }}"
  ]
}
